{
  "name": "xray-image",
  "version": "0.1.0",
  "author": "jing wang",
  "email": "jingwangian@gmail.com",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "description": "This is a small app used to upload/view the image file.",
  "build": {
    "appId": "com.electron.jingwangian",
    "productName": "ImageView",
    "mac": {
      "category": "public.app-category.business"
    },
    "files": [
      "./build/**/*",
      "./electron/main.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "icon": "build/logo512.png"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.66",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "start": "concurrently \"cross-env BROWSER=none REACT_APP_NOTIFICATION=1 yarn react-start\" \"wait-on http://localhost:3000 && electron . --enable-logging\"",
    "build": "REACT_APP_NOTIFICATION=1 yarn react-build && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
